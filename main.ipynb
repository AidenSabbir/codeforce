   "source": [
    "n = list(input())\n",
    "l = len(n)\n",
    "seen = set()\n",
    "pre = ''\n",
    "c = 0\n",
    "for i in range(l):\n",
    "    if n[i] != pre:\n",
    "        if n[i] in seen:\n",
    "            c = 1\n",
    "            break\n",
    "        seen.add(pre)\n",
    "    pre = n[i]       \n",
    "print('NO' if c else 'YES')\n"
   ]
  }
